


<!DOCTYPE html>
<html>
<head>
    <title></title>
    <link href="https://kendo.cdn.telerik.com/themes/6.7.0/default/default-main.css" rel="stylesheet" />
    <script src="js/jquery.min.js"></script>
    <link href="https://kendo.cdn.telerik.com/themes/6.7.0/default/default-main.css" rel="stylesheet" />
    <script src="js/jquery.min.js"></script>
    <script src="js/kendo.all.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link href="https://kendo.cdn.telerik.com/themes/6.7.0/default/default-main.css" rel="stylesheet" />
    <script src="js/jquery.min.js"></script>
    <script src="js/kendo.all.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2023.2.829/js/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-1.12.3.min.js"></script>
    <link rel="stylesheet" href="https://kendo.cdn.telerik.com/themes/6.7.0/default/default-main.css" />
    <script src="https://kendo.cdn.telerik.com/2023.2.829/js/kendo.all.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2023.2.829/mjs/kendo.all.js" type="module"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">


    <script src="js/kendo.all.min.js"></script>

    <style>
        .doc-data {
            height: auto;
            width: auto;
            border: 4px solid red ;
            border-radius:20px;
            color:white;
            background-color: black;
            margin-top: 20px;
            margin-left: 50px;
        }

        .k-listview-content {
            display: flex;
            flex-wrap: wrap;
        }

        #img {
            height: 180px;
            width: 230px;
        }

        #m {
            text-align: center;
        }

        #navbarNav {
            margin-left: 480px;
        }
    </style>

</head>
<body>

    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container">

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <a class="nav-link" href="/Home/index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Home/Medicins">Medicine</a>
                    </li>
                    
                    <li class="nav-item">
                        <a class="nav-link" href="/Home/Bill">Bill</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href="/Home/Customer">Appointment</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href="/Home/ViewCustomer">View Appointment</a>
                    </li>
                  
                </ul>
            </div>
        </div>
    </nav>
    <div class="container-fluid">
        <div class="row justify-content-center">
            <div class="col-3">
                <h2 style="color:navy">Our Doctors</h2>
            </div>
        </div>


    </div>
    <div class="container-fluid">
        <div class="row justify-content-center">
            <div class="col-12">
                <div id="listView">
                </div>
            </div>
        </div>


    </div>

    <script type="text/x-kendo-template" id="template">
        <div class="doc-data">
            <h1>#:dName#</h1>
            <h1>#:dSpecification#</h1>
            <h1>#:dQualification#</h1>
        </div>

    </script>

    <script>
        var menu = @Html.Raw(Json.Serialize(Model));
        $(function () {
            $("#listView").kendoListView({
                dataSource: {
                    data: menu,
                    pageSize: 8
                },
                pageable: {
                    pageSizes: true
                },

                template: kendo.template($("#template").html()),
                pageable: true,
                scrollable: false
            });
        });
    </script>
</body>
</html>
